################  GEE MODEL ###########################
#some problems if we cluster for year (R crashes)
library(geepack)

##2019
modgee19<-geeglm(los8~white+ny_area+
                 d_risky+d_severe+compl+ampt+males+cl_age,data=diadata19,id=ny_hosp_id,family=binomial("logit"),corstr="exchangeable")
summary(modgee19)
names(modgee19)

confint.geeglm <- function(object, parm, level = 0.95, ...) {
  cc <- coef(summary(object))
  mult <- qnorm((1+level)/2)
  citab <- with(as.data.frame(cc),
                cbind(Lower=Estimate-mult*Std.err,
                      Upper=Estimate+mult*Std.err))
  rownames(citab) <- rownames(cc)
  citab[parm,]
}

cbind(coef(summary(modgee19)),confint.geeglm(modgee19))
cbind(coef(summary(backward_model_19)),confint(backward_model_19))   #to make a comparison

##2021
modgee21<-geeglm(los8~white+ny_area+
                 d_risky+d_severe+compl+ampt+males+cl_age,data=diadata21,id=ny_hosp_id,family=binomial("logit"),corstr="exchangeable")
summary(modgee21)
names(modgee21)


cbind(coef(summary(modgee21)),confint.geeglm(modgee21))
cbind(coef(summary(backward_model_21)),confint(backward_model_21))   #to make a comparison
