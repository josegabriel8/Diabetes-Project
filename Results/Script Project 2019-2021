       2,
     paste(round(optimal_cut_1_21$optimal_cutpoint,4),"
(",round(optimal_cut_1_21$specificity*100,1),
           ",",round(optimal_cut_1_21$sensitivity*100,1),")",sep=""),cex=0.65,col="red")

